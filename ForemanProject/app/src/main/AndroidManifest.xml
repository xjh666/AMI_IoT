<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foremanproject">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/foreman_helmet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/foreman_helmet"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BasicActivity"></activity>

    <activity android:name=".activity.AllHosts"></activity>
    </application>

</manifest>